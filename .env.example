# 示例环境配置（节选）

# 资源路径
# VITE_USE_ABSOLUTE_URLS=true
# VITE_ASSETS_BASE_URL=https://cdn.example.com/project/

# GitHub Pages
# VITE_GITHUB_PAGES=true
# VITE_GITHUB_REPOSITORY=username/repository

# —— 版权信息（共享组件 CopyrightInfo 使用） ——
VITE_COPYRIGHT_AUTHOR=Your Company
VITE_COPYRIGHT_EMAIL=contact@example.com
VITE_COPYRIGHT_WEBSITE=https://example.com
VITE_COPYRIGHT_TOOL_NAME=Your Tool Name
VITE_COPYRIGHT_YEAR=2025

# 可选开关（按需开启/关闭）
VITE_COPYRIGHT_SHOW_SYMBOL=true
VITE_COPYRIGHT_SHOW_CONTACT=true
VITE_COPYRIGHT_SHOW_WEBSITE=true
VITE_COPYRIGHT_SHOW_POWERED_BY=true
VITE_COPYRIGHT_SHOW_LICENSE=false
VITE_COPYRIGHT_SHOW_ATTRIBUTION=false

# 其他可选字段
# VITE_COPYRIGHT_LICENSE=MIT
# VITE_COPYRIGHT_ATTRIBUTION=Attribution text here
# VITE_COPYRIGHT_CLASSNAME=custom-class